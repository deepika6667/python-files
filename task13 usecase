      "source": [
        "# -------------------------------------------------------------\n",
        "# Sales Forecasting using Moving Averages, Trend & Plotly Chart\n",
        "# -------------------------------------------------------------\n",
        "\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import plotly.express as px\n",
        "\n",
        "# Step 1: Create historical sales data (24 months)\n",
        "dates = pd.date_range(start=\"2023-01-01\", periods=24, freq='M')\n",
        "sales = [200 + np.random.randint(-20, 30) + i * 5 for i in range(24)]  # Slight upward trend\n",
        "df = pd.DataFrame({'Month': dates, 'Sales': sales})\n",
        "\n",
        "# Step 2: Compute Moving Averages\n",
        "df['3-Month MA'] = df['Sales'].rolling(window=3).mean()\n",
        "df['6-Month MA'] = df['Sales'].rolling(window=6).mean()\n",
        "\n",
        "# Step 3: Identify Trend using Linear Regression (NumPy)\n",
        "x = np.arange(len(df))\n",
        "y = df['Sales'].values\n",
        "\n",
        "coefficients = np.polyfit(x, y, 1)  # Linear fit\n",
        "trend = np.poly1d(coefficients)\n",
        "df['Trend'] = trend(x)\n",
        "\n",
        "# Step 4: Forecast Next Monthâ€™s Sales\n",
        "next_month_index = len(df)\n",
        "forecast_value = trend(next_month_index)\n",
        "next_month = df['Month'].iloc[-1] + pd.DateOffset(months=1)\n",
        "\n",
        "print(\"------------------------------------------------\")\n",
        "print(\"ðŸ“Š Sales Forecasting Result\")\n",
        "print(\"------------------------------------------------\")\n",
        "print(f\"Forecasted Sales for Next Month ({next_month.strftime('%B %Y')}): {forecast_value:.2f}\")\n",
        "print(\"------------------------------------------------\")\n",
        "\n",
        "# Step 5: Append Forecast Data\n",
        "forecast_df = pd.DataFrame({\n",
        "    'Month': [next_month],\n",
        "    'Sales': [np.nan],\n",
        "    '3-Month MA': [np.nan],\n",
        "    '6-Month MA': [np.nan],\n",
        "    'Trend': [forecast_value]\n",
        "})\n",
        "df_forecast = pd.concat([df, forecast_df], ignore_index=True)\n",
        "\n",
        "# Step 6: Plot using Plotly\n",
        "fig = px.line(df_forecast, x='Month', y=['Sales', '3-Month MA', '6-Month MA', 'Trend'],\n",
        "              title='Sales Forecast with Moving Averages and Trend Line',\n",
        "              labels={'value': 'Sales Amount', 'variable': 'Metric'},\n",
        "              markers=True)\n",
        "\n",
        "# Highlight forecast point\n",
        "fig.add_scatter(x=[next_month], y=[forecast_value],\n",
        "                mode='markers+text',\n",
        "                name='Forecasted Sales',\n",
        "                text=[f\"{forecast_value:.2f}\"],\n",
        "                textposition=\"top center\",\n",
        "                marker=dict(size=10, color='red'))\n",
        "\n",
        "fig.show()"
      ]
    }
  ]
}